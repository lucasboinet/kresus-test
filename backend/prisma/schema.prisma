datasource db {
  provider = "sqlite"
  url      = "file:./todolist.db"
}

generator client {
  provider = "prisma-client-js"
  // previewFeatures = []
}

model Todo {
  id    Int     @id @default(autoincrement())
  title String  @unique
  content  String
  priority String
  executionDate DateTime?
}
